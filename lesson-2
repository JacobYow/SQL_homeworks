--1 CREATE TABLE Employees (
    EmpID INT,
    Name VARCHAR(50),
    Salary DECIMAL(10, 2)
)
select * from Employees

--2 INSERT INTO Employees (EmpID, Name, Salary)
VALUES (1, 'Alice Johnson', 60000.00),
  (2, 'Bob Smith', 72000.50),
  (3, 'Charlie Davis', 55000.75)

--3 UPDATE Employees
SET Salary = 65000.00
WHERE EmpID = 1

  --4 DELETE FROM Employees
WHERE EmpID = 2

